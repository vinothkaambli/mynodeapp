    "express-session": "^1.10.4",
    "gitkitclient": "0.0.7",
    "googleapis": "^34.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.10",
    "material-ui": "^0.20.1",
