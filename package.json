    "build": "echo 'building app' && npm run babelize",
    "webpack": "echo '=> running webpack' && webpack --watch"
  },
  "jest": {
    "verbose": true,
    "coverageDirectory": "coverage/frontEnd",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/controller/",
      "/__test__/",
      "/test/",
      "/lib/",
      "/config/",
      "/.build/"
    ],
    "roots": [
      "test/unit/"
    ],
    "moduleNameMapper": {
      "\\.(css|svg)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/test/testReactAdapterSetup.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "es6"
    ]
  },
